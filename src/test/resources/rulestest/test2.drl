package test
import com.shj.entity.XFact
import com.shj.entity.FactResult
rule "Using Testing2"
when
    $XFact:LHS(this["name"] contains "網易",
             this["a"] matches  "心.*",
             this["email"] matches "^[A-Za-z]+$")
    eval( true )
then
    FactResult fr = new FactResult("Using Testing2");
    fr.put("test1", $XFact.get("name"));
    insert(fr);
end